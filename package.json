    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^26.6.3",
    "prettier": "^3.0.3",
    "typescript": "^4.2.3",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
